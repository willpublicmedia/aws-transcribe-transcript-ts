{
  "name": "aws-transcribe-transcript-ts",
  "version": "0.0.0",
  "description": "A typescript port of purdy/aws-transcribe-transcript",
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "webpack --mode=development && npm run tidy",
    "build:prod": "webpack --mode=production --node-env=production && npm run tidy",
    "clean": "rm -rf ./dist/",
    "test": "jest",
    "tidy": "rm -rf ./dist/@types/tests && find ./dist -name '*.d.ts' -exec mv -t ./dist/@types/ {} + && rm -rf ./dist/@types/src"
  },
  "main": "./dist/transcriptFormatter.js",
  "types": "./dist/transcriptFormatter.d.ts",
  "files": [
    "dist"
  ],
  "type": "commonjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/willpublicmedia/aws-transcribe-transcript-ts.git"
  },
  "author": "Illinois Public Media",
  "license": "NCSA",
  "bugs": {
    "url": "https://github.com/willpublicmedia/aws-transcribe-transcript-ts/issues"
  },
  "homepage": "https://github.com/willpublicmedia/aws-transcribe-transcript-ts#readme",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.23.0",
    "@jest/globals": "29.7.0",
    "@types/node": "^20.8.2",
    "@webpack-cli/generators": "^3.0.7",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}